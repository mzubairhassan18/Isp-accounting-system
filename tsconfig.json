{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "/*": ["*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "images.d.ts"],
  "exclude": ["node_modules"]
}
